def parimpar():
    sumapar=0
    sumaimpar=0
    for cont in range(1,6):
        num=int(input("dame un numero:"))
        if(num%2==0):
            print("es par")
            sumapar=sumapar+num
        else:
            print("es impar")
            sumaimpar=sumaimpar+num
    print("la suma de los pares es: " +str(sumapar))
    print("la suma de los impares es: " +str(sumaimpar))
parimpar()
