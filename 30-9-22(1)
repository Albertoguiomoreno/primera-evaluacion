# Write Python 3 code in this online editor and run it.
#pide dos numeros y calcula la media
def calcula_media():
    n1=0
    n2=0
    n1=int(input("dime un numero"))
    n2=int(input("dime un numero"))
    suma=n1+n2
    media=suma/2
    print(media)
calcula_media()
